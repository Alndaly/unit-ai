{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "include": [
    "next-env.d.ts",
  "next.config.js",
  "**/*.ts",
  "**/*.tsx",
  ".next/types/**/*.ts",
  "dist/types/**/*.ts"
],
"exclude": ["node_modules"],
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true
  }
}
